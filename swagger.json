{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Swagger Petstories",
    "description": "API for pets",
    "termsOfService": "http://swagger.io/terms/",
    "contact": {
      "name": "Cool API Team"
    },
    "license": {
      "name": "We are freedom (WAF)"
    }
  },
  "host": "pets-zywq.onrender.com",
  "basePath": "/api",
  "schemes": ["https"],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "tags": [
    {
      "name": "users",
      "description": "API for users authenticate"
    },
    {
      "name": "notices",
      "description": "API for notices"
    }
  ],

  "securityDefinitions": {
    "BearerAuth": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "description": "Bearer token in the format 'Bearer <token>'"
    }
  },
  "paths": {
    "/users/register": {
      "post": {
        "summary": "Create a new user in the data base.  Duplicates are denied",
        "operationId": "addNewUser",
        "produces": ["application/json"],
        "tags": ["users"],
        "parameters": [
          {
            "name": "users",
            "in": "body",
            "description": "Registration of new user",
            "required": true,
            "schema": {
              "$ref": "#/definitions/NewUserReq"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "User response",
            "schema": {
              "$ref": "#/definitions/NewUserRes"
            }
          },
          "409": {
            "description": "Email already exists / wrong data"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/users/login": {
      "post": {
        "summary": "Login a user from the data base.",
        "operationId": "loginUser",
        "produces": ["application/json"],
        "tags": ["users"],
        "parameters": [
          {
            "name": "users",
            "in": "body",
            "description": "Login of user",
            "required": true,
            "schema": {
              "$ref": "#/definitions/LoginUserReq"
            }
          }
        ],

        "responses": {
          "200": {
            "description": "User response",
            "schema": {
              "$ref": "#/definitions/UserRes"
            }
          },
          "404": {
            "description": "Email not found"
          },
          "409": {
            "description": "Wrong data"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/users/logout": {
      "post": {
        "summary": "Logout of a user.",
        "operationId": "logoutUser",
        "produces": ["application/json"],
        "tags": ["users"],
        "parameters": [
          {
            "name": "Logout",
            "in": "header",
            "type": "string",
            "description": "Bearer token must have in the format 'Bearer {token}'",
            "allowEmptyValue": false,
            "readOnly": true
          }
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "User response",
            "schema": {
              "$ref": "#/definitions/LogoutUserRes"
            }
          },
          "401": {
            "description": "Unauthorized. Invalid token / No such user, login or register"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/users/current": {
      "get": {
        "summary": "Get current user",
        "operationId": "getCurrentUser",
        "produces": ["application/json"],
        "tags": ["users"],
        "parameters": [
          {
            "name": "Current user",
            "description": "Fetch of current user. Bearer token must have in the format 'Bearer {token}'"
          }
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "User response",
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "$ref": "#/definitions/UserRes"
              }
            }
          },
          "401": {
            "description": "Unauthorized. Invalid token / No such user, login or register"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/users/update": {
      "patch": {
        "summary": "Updates a user",
        "tags": ["users"],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "name",
            "in": "formData",
            "type": "string",
            "required": false,
            "description": "The name of the user"
          },
          {
            "name": "email",
            "in": "formData",
            "type": "string",
            "required": false,
            "description": "The email of the user"
          },
          {
            "name": "phone",
            "in": "formData",
            "type": "string",
            "required": false,
            "description": "The phone of the user"
          },
          {
            "name": "city",
            "in": "formData",
            "type": "string",
            "required": false,
            "description": "The city of the user"
          },
          {
            "name": "birthday",
            "in": "formData",
            "type": "string",
            "required": false,
            "description": "The birthday of the user"
          },
          {
            "name": "file",
            "in": "formData",
            "type": "file",
            "required": false,
            "description": "The avatar image of the user"
          }
        ],
        "responses": {
          "200": {
            "description": "User updated successfully",
            "schema": {
              "$ref": "#/definitions/UserRes"
            }
          },
          "400": {
            "description": "No data"
          },
          "401": {
            "description": "Unauthorized. Invalid token / No such user, login or register"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/notices/getall": {
      "get": {
        "summary": "Get all notices",
        "operationId": "getAllNotices",
        "produces": ["application/json"],
        "tags": ["notices"],

        "responses": {
          "200": {
            "description": "Notices response",
            "schema": {
              "$ref": "#/definitions/NoticesAllRes"
            }
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/notices/noticesuser": {
      "get": {
        "summary": "Get all notices of current user",
        "operationId": "getAllNoticesCurrentUser",
        "produces": ["application/json"],
        "tags": ["notices"],
        "parameters": [
          {
            "name": "notices",
            "description": "Get all notices of current user. Bearer token must have in the format 'Bearer {token}'"
          }
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Notices response",
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "$ref": "#/definitions/NoticesUserRes"
              }
            }
          },
          "401": {
            "description": "Unauthorized. Invalid token / No such user, login or register"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/notices/search": {
      "get": {
        "summary": "Search notices",
        "operationId": "searchNotices",
        "produces": ["application/json"],
        "tags": ["notices"],
        "parameters": [
          {
            "name": "word",
            "in": "query",
            "type": "string",
            "description": "Search notices by the words of the title "
          },
          {
            "name": "category",
            "in": "query",
            "type": "string",
            "description": "Search notices by category"
          },
          {
            "name": "page",
            "in": "query",
            "type": "string",
            "description": "Page"
          },
          {
            "name": "limit",
            "in": "query",
            "type": "string",
            "description": "Limit"
          }
        ],
        "responses": {
          "200": {
            "description": "Search results",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/NoticesSearchRes"
              }
            }
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/notices/{id}": {
      "get": {
        "summary": "Get  notice by ID",
        "operationId": "getOneNotice",
        "produces": ["application/json"],
        "tags": ["notices"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Fetch one notice",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Notices response",
            "schema": {
              "$ref": "#/definitions/NoticesAllRes"
            }
          },
          "400": {
            "description": "Invalid ID"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/notices/add": {
      "post": {
        "summary": "Create a new notice.",
        "operationId": "addNewNotice",
        "produces": ["application/json"],
        "tags": ["notices"],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "category",
            "in": "formData",
            "type": "string",
            "description": "The notification Category must be one of the following: 'your pet/sell/lost/in good hands'",
            "required": true
          },
          {
            "name": "name",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "The name of pet"
          },
          {
            "name": "birthday",
            "in": "formData",
            "type": "string",
            "required": false,
            "description": "Birthday of the pet"
          },
          {
            "name": "type",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "Type of pet (dog, cat e.g.)"
          },
          {
            "name": "comments",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "Comments of notice"
          },
          {
            "name": "file",
            "in": "formData",
            "type": "file",
            "required": false,
            "description": "The avatar image of the pet"
          },
          {
            "name": "title",
            "in": "formData",
            "type": "string",
            "required": false,
            "description": "Title of notice"
          },
          {
            "name": "sex",
            "in": "formData",
            "type": "string",
            "required": false,
            "description": "Sex of pet"
          },
          {
            "name": "location",
            "in": "formData",
            "type": "string",
            "required": false,
            "description": "Location of pet"
          },
          {
            "name": "price",
            "in": "formData",
            "type": "string",
            "required": false,
            "description": "Price of pet (Number)"
          }
        ],
        "responses": {
          "200": {
            "description": "User response",
            "type": "object",
            "schema": {
              "oneOf": [
                { "$ref": "#/definitions/NoticesResUser" },
                { "$ref": "#/definitions/NoticesResPet" }
              ]
            }
          },
          "400": {
            "description": "Field CATEGORY is required and must have 'your pet' or 'sell' or 'lost/found' or 'in good hands' / Fields CATEGORY, NAME, TYPE and COMMENTS are required"
          },
          "401": {
            "description": "You do not have access to the database"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/notices/delnotice/{id}": {
      "delete": {
        "summary": "Delete your own notice",
        "operationId": "delNotice",
        "produces": ["application/json"],
        "tags": ["notices"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Bearer token must have in the format 'Bearer {token}'",
            "type": "string",
            "required": true
          }
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "User response",
            "schema": {
              "$ref": "#/definitions/DeleteNotice"
            }
          },
          "400": {
            "description": "Invalid ID"
          },
          "401": {
            "description": "Unauthorized. Invalid token / No such user, login or register"
          },
          "404": {
            "description": "Notice not found"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/notices/delpet/{id}": {
      "delete": {
        "summary": "Delete your own pet",
        "operationId": "delPet",
        "produces": ["application/json"],
        "tags": ["notices"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Bearer token must have in the format 'Bearer {token}'",
            "type": "string",
            "required": true
          }
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "User response",
            "schema": {
              "$ref": "#/definitions/DeletePet"
            }
          },
          "400": {
            "description": "Invalid ID"
          },
          "401": {
            "description": "Unauthorized. Invalid token / No such user, login or register"
          },
          "404": {
            "description": "The pet does not exist"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/notices/favorite/{id}": {
      "patch": {
        "summary": "Add favorite notice",
        "operationId": "favorite",
        "produces": ["application/json"],
        "tags": ["notices"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Bearer token must have in the format 'Bearer {token}'",
            "type": "string",
            "required": true
          }
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "User response",
            "schema": {
              "example": "{ID} added notice to favorite"
            }
          },
          "400": {
            "description": "Invalid ID"
          },
          "401": {
            "description": "Unauthorized. Invalid token / No such user, login or register"
          },
          "404": {
            "description": "Notice not found"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/notices/favoritedelete/{id}": {
      "patch": {
        "summary": "Delete your favorite notice",
        "operationId": "delFavNotice",
        "produces": ["application/json"],
        "tags": ["notices"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Bearer token must have in the format 'Bearer {token}'",
            "type": "string",
            "required": true
          }
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "User response",
            "schema": {
              "example": "{ID} deleted notice from favorite"
            }
          },
          "400": {
            "description": "Invalid ID"
          },
          "401": {
            "description": "Unauthorized. Invalid token / No such user, login or register"
          },
          "404": {
            "description": "Notice not found"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/notices/getfavorites": {
      "get": {
        "summary": "Favorites list of current user",
        "operationId": "searchNews",
        "produces": ["application/json"],
        "tags": ["notices"],
        "parameters": [
          {
            "description": "Bearer token must have in the format 'Bearer {token}'",
            "type": "string"
          }
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Search results",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/NoticesFavoriteRes"
              }
            }
          },
          "401": {
            "description": "Unauthorized. Invalid token / No such user, login or register"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/news/getall": {
      "get": {
        "summary": "Search news",
        "operationId": "searchNews",
        "produces": ["application/json"],
        "tags": ["news"],
        "parameters": [
          {
            "name": "word",
            "in": "query",
            "type": "string",
            "description": "Search news by the words of the title "
          },

          {
            "name": "page",
            "in": "query",
            "type": "string",
            "description": "Page"
          },
          {
            "name": "limit",
            "in": "query",
            "type": "string",
            "description": "Limit"
          }
        ],
        "responses": {
          "200": {
